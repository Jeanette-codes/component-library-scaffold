{
  "name": "component-library-scaffold",
  "version": "0.0.1",
  "description": "Produce universal library with webpack and es6",
  "main": "lib/library.js",
  "scripts": {
    "build": "webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --compilers js:babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "3.5.0",
    "classnames": "^2.2.3",
    "css-loader": "^0.28.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.0",
    "mocha": "3.3.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-easy-import": "^2.0.0",
    "postcss-loader": "^2.0.5",
    "postcss-modules": "^0.7.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "webpack": "2.5.1",
    "yargs": "8.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/krasimir/webpack-library-starter.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Jeremy Hitchcock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fender-guy/component-library-scaffold/issues"
  },
  "homepage": "https://github.com/fender-guy/component-library-scaffold"
}
